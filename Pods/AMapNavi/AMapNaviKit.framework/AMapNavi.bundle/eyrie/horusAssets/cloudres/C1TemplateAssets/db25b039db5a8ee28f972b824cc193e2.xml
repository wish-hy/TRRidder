<!--公交详情页公交站点缩略图气泡样式-->
<xml>
  <params>
    <param useCommonParser='true' direction='topCenter' anchorRatio='(0.5,1.0)'>
      <subparam
        id='firstimage'
        width='110px'
        height='123px'
        background-image='200024'
      ></subparam>

      <subparam id='firstdiv' top='17px' left='21px'></subparam>
    </param>

    <param useCommonParser='true' direction='rightTop' anchorRatio='(0.0,1.0)'>
      <subparam
        id='firstimage'
        width='124px'
        height='127px'
        background-image='200025'
      ></subparam>

      <subparam id='firstdiv' left='35px' bottom='42px'></subparam>
    </param>

    <param
      useCommonParser='true'
      direction='rightBottom'
      anchorRatio='(0.0,0.0)'
    >
      <subparam
        id='firstimage'
        width='124px'
        height='127px'
        background-image='200025'
        backgroundtransform='rotate(180,0,0)'
      ></subparam>

      <subparam id='firstdiv' top='41px' left='35px'></subparam>
    </param>

    <param
      useCommonParser='true'
      direction='bottomCenter'
      anchorRatio='(0.5,0.0)'
    >
      <subparam
        id='firstimage'
        width='110px'
        height='129px'
        background-image='200024'
        backgroundtransform='rotate(180,0,0)'
      ></subparam>

      <subparam id='firstdiv' top='43px' left='21px'></subparam>
    </param>

    <param
      useCommonParser='true'
      direction='leftBottom'
      anchorRatio='(1.0,0.0)'
    >
      <subparam
        id='firstimage'
        width='124px'
        height='127px'
        background-image='200025'
        backgroundtransform='rotate(180,180,0)'
      ></subparam>

      <subparam id='firstdiv' top='41px' right='35px'></subparam>
    </param>

    <param useCommonParser='true' direction='leftTop' anchorRatio='(1.0,1.0)'>
      <subparam
        id='firstimage'
        width='124px'
        height='127px'
        background-image='200025'
        backgroundtransform='rotate(0,180,0)'
      ></subparam>

      <subparam id='firstdiv' right='35px' bottom='41px'></subparam>
    </param>
  </params>

  <div style='position: relative'>
    <div id='firstimage' style='position: relative'></div>

    <div id='firstdiv' style='position: absolute'>
      <img
        style='width: 68px; height: 68px; border-radius: 18px'
        src='{label}'
        syncloadCache='false'
        fastmode='false'
      ></img>
    </div>
  </div>
</xml>
