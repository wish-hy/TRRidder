<!--公共感知优化专项二期-200032-->
<xml>
  <params>
    <param useCommonParser='true' direction='bottomCenter' anchorOffset='(0,0)'>
      <subparam
        id='backimage'
        background-image='200034'
        backgroundstretch='47% 64% 35% 25% 47% 25% 35% 64'
        padding='38px 30px 28px 31px'
      ></subparam>
    </param>
  </params>

  <div id='backimage' style='left: 0; top: 0px' syncload='true'>
    <div style='display: flex; flex-direction: row; align-items: center; position: relative'>
      <div style='display: flex; line-height: 28px; margin-right: 9px; flex-direction: column; align-items: flex-start'>
        <label
          style='align-self: flex-start; font-size: 26px; color: @Color_Hue170_H8; position: relative'
          text='{subLabel}'
        ></label>
      </div>

      <img
        style='width: 9px; height: 16px; align-self: center; margin-top: -2px'
        src='30025'
        syncload='true'
      ></img>
    </div>
  </div>
</xml>
