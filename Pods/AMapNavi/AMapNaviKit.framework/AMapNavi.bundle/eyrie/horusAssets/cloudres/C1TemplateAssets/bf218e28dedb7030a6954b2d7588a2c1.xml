<!--兴趣点icon纹理点击态-->
<xml>
  <params>
    <param useCommonParser='true' direction='topCenter' anchorOffset='(0,-18)'>
      <subparam id='icon' width='80px' height='80px'></subparam>

      <subparam id='background' width='112px' height='112px'></subparam>
    </param>
  </params>

  <div
    id='background'
    style='display: flex; background-image: {backImg}; flex-direction: row; align-items: center; justify-content: center'
  >
    <img id='icon' style='margin-bottom: 16px' src='{iconId}'></img>
  </div>
</xml>
