<!--FestivalToolGate_entrance-->
<xml>
  <params>
    <param direction='rightTop' anchorOffset='(36,15)'>
      <subparam
        id='background'
        background-image='https://faas-image.amap.com/shaolin/images/FestivalToolGate_entrance_fast/FestivalToolGate_entrance_right_fast.png?d=202304181544'
        backgroundstretch='52% 34% 39% 50%'
        padding='36px 48px 28px 70px'
      ></subparam>

      <subparam id='contain' align-self='flex-start'></subparam>

      <subparam id='entrance' align-items='flex-start'></subparam>

      <subparam id='nameLable' align-self='flex-start'></subparam>

      <subparam id='image' left='0px'></subparam>
    </param>

    <param direction='leftTop' anchorOffset='(36,15)'>
      <subparam
        id='background'
        background-image='https://faas-image.amap.com/shaolin/images/FestivalToolGate_entrance_fast/FestivalToolGate_entrance_left_fast.png?d=202304181544'
        backgroundstretch='52% 51% 39% 34%'
        padding='36px 68px 28px 50px'
      ></subparam>

      <subparam id='contain' align-self='flex-end'></subparam>

      <subparam id='entrance' align-items='flex-end'></subparam>

      <subparam id='nameLable' align-self='flex-end'></subparam>

      <subparam id='image' right='0px'></subparam>
    </param>
  </params>

  <div style='display: flex; align-items: flex-start; flex-direction: column'>
    <div
      id='contain'
      style='display: flex; position: relative; flex-direction: row'
    >
      <div style='display: flex; position: relative; top: 0px; left: 0px; align-items: flex-start; flex-direction: row'>
        <div id='background' style='top: 0px; left: 0' syncload='true'>
          <div
            id='entrance'
            style='display: flex; position: relative; flex-direction: row; font-weight: bold; line-height: 24px; white-space: nowrap'
          >
            <label
              style='color: #2e2e2e; font-size: 20px; margin-right: 0px; font-weight: bold; align-self: flex-start'
              text='入口'
            ></label>
          </div>
        </div>
      </div>

      <img
        id='image'
        style='width: 70px; height: 88px; position: absolute; align-self: flex-start'
        src='https://faas-image.amap.com/shaolin/images/FestivalToolGate_entrance_fast/FestivalToolGate_entrance_fast@2x.png?d=202304181544'
        syncload='true'
        syncloadCache='false'
        fastmode='false'
      ></img>
    </div>

    <label
      id='nameLable'
      style='position: relative; overflow: hidden; text-overflow: ellipsis; line-height: 22px; white-space: nowrap; color: #000000; font-size: 18px; font-weight: bold; text-stroke: 2px #FFFFFF'
      text='{tollGateName}'
    ></label>
  </div>
</xml>