<!--顺路搜普通态水滴气泡-->
<xml>
  <params>
    <param useCommonParser='true' direction='rightBottom' anchorOffset='(33,0)'>
      <subparam id='maindiv' flex-direction='row'></subparam>

      <subparam id='labeldiv' align-items='flex-start'></subparam>

      <subparam id='firstlab' align-self='flex-start'></subparam>

      <subparam id='secondlab' align-self='flex-start'></subparam>
    </param>

    <param useCommonParser='true' direction='leftBottom' anchorOffset='(33,0)'>
      <subparam id='maindiv' flex-direction='row-reverse'></subparam>

      <subparam
        id='labeldiv'
        align-items='flex-end'
        justify-content='flex-end'
      ></subparam>

      <subparam id='firstlab' align-self='flex-end'></subparam>

      <subparam id='secondlab' align-self='flex-end'></subparam>
    </param>

    <param useCommonParser='true' direction='topCenter' anchorOffset='(0,56)'>
      <subparam id='maindiv' flex-direction='column'></subparam>

      <subparam
        id='labeldiv'
        align-items='center'
        justify-content='center'
      ></subparam>

      <subparam id='firstlab' align-self='center'></subparam>

      <subparam id='secondlab' align-self='center'></subparam>
    </param>
  </params>

  <div>
    <div
      id='maindiv'
      style='align-items: flex-start; justify-content: flex-end'
    >
      <img
        style='width: 66px; height: 66px; margin-right: 3px; align-self: center'
        src='{facilitiesIcon}'
        syncload='true'
        syncloadCache='false'
        fastmode='false'
      ></img>

      <div
        id='labeldiv'
        style='display: flex; position: relative; flex-direction: column'
      >
        <div style='display: flex; align-items: flex-start; flex-direction: column; margin-top: 4px; font-weight: bold; white-space: nowrap'>
          <label
            id='firstlab'
            style='position: relative; margin-left: 1px; overflow: hidden; text-overflow: ellipsis; line-height: 30px; color: {namecolor}; font-size: 24px; flex-direction: row; max-width: 245px; text-layout-amap: Y; text-align: right; font-weight: bold; text-stroke: 2px {strokeColor}; line-clamp: 1'
            text='{name}'
          ></label>

          <div style='display: flex; position: relative; align-items: flex-start; flex-direction: row; margin-top: 2px; border-radius: 8px; background-color: {etaInfoBgColor}; line-height: 25px'>
            <label
              id='secondlab'
              style='overflow: hidden; text-overflow: ellipsis; color: {etaInfoColor}; font-size: 20px; line-height: 25px; margin: 1px 5px'
              text='{etaInfo}'
            ></label>
          </div>
        </div>
      </div>
    </div>
  </div>
</xml>
