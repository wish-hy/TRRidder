<!--限速电子眼动态纹理样式-->
<xml>
  <params>
    <param
      useCommonParser='true'
      direction='leftTop'
      anchorOffset='(2,18)'
      anchorOffsetHD='(12,18)'
    >
      <subparam id='firstdiv' top='25px' right='40px'></subparam>
    </param>

    <param
      useCommonParser='true'
      direction='rightTop'
      anchorOffset='(2,18)'
      anchorOffsetHD='(12,18)'
    >
      <subparam id='firstdiv' top='25px' left='40px'></subparam>
    </param>

    <param
      useCommonParser='true'
      direction='leftBottom'
      anchorOffset='(2,18)'
      anchorOffsetHD='(12,10)'
    >
      <subparam id='firstdiv' top='40px' right='40px'></subparam>
    </param>

    <param
      useCommonParser='true'
      direction='rightBottom'
      anchorOffset='(2,18)'
      anchorOffsetHD='(12,10)'
    >
      <subparam id='firstdiv' top='40px' left='40px'></subparam>
    </param>
  </params>

  <div style='display: flex; flex-direction: row; align-items: center'>
    <img style='width: 142px; height: 136px' src='{popResId}'></img>

    <div
      id='firstdiv'
      style='display: flex; width: 68px; height: 68px; position: absolute; align-items: center; justify-content: center'
    >
      <label
        style='color: #222222; font-size: {fontSize}; font-weight: bold'
        text='{maxspeed}'
      ></label>
    </div>
  </div>
</xml>
