<!--自定义路线拖拽路线-->
<xml>
  <params>
    <param useCommonParser='true' direction='topCenter' anchorOffset='(0,0)'>
      <subparam
        id='backimage'
        background-image='204104'
        backgroundstretch='39% 67% 52% 25% 39% 25% 52% 67%'
        padding='27px 47px 39px 45px'
        top='-10px'
      ></subparam>
    </param>

    <param useCommonParser='true' direction='bottomCenter' anchorOffset='(0,0)'>
      <subparam
        id='backimage'
        background-image='204105'
        backgroundstretch='52% 67% 39% 25% 52% 25% 39% 67%'
        padding='38px 47px 28px 45px'
        top='10px'
      ></subparam>
    </param>
  </params>

  <div id='backimage' style='left: 0'>
    <div style='display: flex; position: relative; align-items: flex-start; flex-direction: row; line-height: 40px'>
      <label
        style='color: @Color_Text_L1; font-size: 28px; font-weight: normal; margin: 0px 0px; margin-right: 0px; align-self: flex-start'
        text='拖拽'
      ></label>
    </div>
  </div>
</xml>
