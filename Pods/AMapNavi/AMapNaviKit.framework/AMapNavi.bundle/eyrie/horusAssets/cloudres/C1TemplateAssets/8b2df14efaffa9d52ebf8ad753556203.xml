<!--通行证气泡-->
<xml>
  <params>
    <param useCommonParser='true' direction='rightTop' anchorOffset='(17,22)'>
      <subparam
        id='backimage'
        background-image='231319'
        padding='25px 31px 49px 44px'
        backgroundstretch='29% 42% 53% 44%'
      ></subparam>
    </param>

    <param useCommonParser='true' direction='leftTop' anchorOffset='(17,22)'>
      <subparam
        id='backimage'
        background-image='231318'
        padding='26px 44px 49px 31px'
        backgroundstretch='29% 42% 53% 44%'
      ></subparam>
    </param>

    <param useCommonParser='true' direction='centerTop' anchorOffset='(0,18)'>
      <subparam
        id='backimage'
        background-image='231323'
        padding='23px 31px 48px 33px'
        backgroundstretch='40% 60% 50% 25% 50% 25% 40% 60%'
      ></subparam>
    </param>

    <param useCommonParser='true' direction='rightBottom' anchorOffset='(17,22)'>
      <subparam
        id='backimage'
        background-image='231321'
        padding='40px 27px 32px 44px'
        backgroundstretch='37% 42% 53% 44%'
      ></subparam>
    </param>

    <param useCommonParser='true' direction='leftBottom' anchorOffset='(17,22)'>
      <subparam
        id='backimage'
        background-image='231320'
        padding='42px 44px 32px 31px'
        backgroundstretch='53% 44% 29% 42%'
      ></subparam>
    </param>

    <param useCommonParser='true' direction='centerBottom' anchorOffset='(0,18)'>
      <subparam
        id='backimage'
        background-image='231322'
        padding='40px 31px 30px 31px'
        backgroundstretch='40% 60% 50% 25% 50% 25% 40% 60%'
      ></subparam>
    </param>
  </params>

  <div>
    <div>
      <div id='backimage' syncload='true'>
        <div style='display: flex; flex-direction: row; align-items: center; position: relative'>
          <div style='display: flex; white-space: nowrap; margin-right: 7px; flex-direction: column; align-items: flex-start'>
            <label
              style='align-self: flex-start; font-weight: bold; font-size: 28px; color: #ec0e3e; line-height: 33px; position: relative'
              text='需通行证'
            ></label>

            <label
              style='align-self: flex-start; font-weight: normal; font-size: 24px; color: #99000000; line-height: 28px; text-overflow: ellipsis; overflow: hidden; margin-top: 2px; position: relative; max-width: 153px; line-clamp: 1'
              text='{city}'
            ></label>
          </div>

          <img
            style='width: 24px; height: 24px; align-self: center; margin-top: 1px'
            src='30025'
            syncload='true'
            syncloadCache='false'
            fastmode='false'
          ></img>
        </div>
      </div>
    </div>
  </div>
</xml>
