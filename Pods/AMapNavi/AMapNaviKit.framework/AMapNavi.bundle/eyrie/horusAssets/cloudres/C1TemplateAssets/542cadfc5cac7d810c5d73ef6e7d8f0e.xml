<!--POI点击纹理-->
<xml>
  <params>
    <param useCommonParser="true" direction="bottomCenter" anchorOffset="(0, 132)"></param>
  </params>
  <div style="display:flex;flex-direction:column;">
    <div style="display:flex;flex-direction:row;justify-content:center;align-items:center;">
      <image src="{iconId}" style="width:96px; height:132px;"></image>
    </div>
    <div style="flex-direction:column;align-items:center;justify-content:center;padding-top:4px;">
      <label text="{name}" style="max-width:308px;line-clamp:2;align-self:center;text-align:center;font-size:{fontSize}; font-weight:bold;color:{color};text-stroke:{strokeWidth} {strokeColor};"></label>
    </div>
  </div>
</xml>
