<!--推荐停车场信息bubble普通态-->
<xml>
  <params>
    <param useCommonParser="true" direction="leftCenter" anchorOffset="(-30,0)">
      <subparam id="firstdiv" align-items="flex-end"></subparam>
      <subparam id="firstlabel" text-align="right"></subparam>
    </param>
    <param useCommonParser="true" direction="rightCenter" anchorOffset="(-30,0)">
      <subparam id="firstdiv" align-items="flex-start"></subparam>
      <subparam id="firstlabel" text-align="left"></subparam>
    </param>
  </params>
  <div style="display: flex; flex-direction: row;">
    <div id="firstdiv" style="display: flex; flex-direction: column;">
      <label id="firstlabel" style="line-clamp: 2; font-size: 28px; color: {namecolor}; line-height: 36px;text-layout-amap:Y;max-width: 196px; font-weight: bold;text-stroke: 2px {strokecolor}" text="{name}"></label>
      <div style="display: {info}; height: 38px; background-color: {bgcolor}; border-radius: 8px; align-items: center;flex-direction: row">
        <div style="width: 30px;height:30px;align-items:center;background-color: #3377E3; border-radius: 8px;margin-left: 4px;margin-right: 4px;flex-direction: column">
          <label style="font-weight: bold;font-size: 24px; color: #ffffff" text="{rank}"></label>
        </div>
        <label style="font-weight: normal; font-size: 24px; color: {percentcolor};margin-right: 4px;line-clamp: 1" text="{percent}"></label>
      </div>
    </div>
  </div>
</xml>
