<!--电子眼静态纹理拼接样式-->
<xml>
  <params>
    <param
      useCommonParser='true'
      direction='leftTop'
      anchorOffset='(2,18)'
      anchorOffsetHD='(12,18)'
    >
      <subparam
        id='firstimage'
        margin-right='10px'
        margin-left='0px'
      ></subparam>

      <subparam id='firstdiv' right='104px' top='13px'></subparam>

      <subparam id='seconddiv' right='12px'></subparam>

      <subparam id='labNormalImage' right='40px' top='25px'></subparam>

      <subparam id='thirddiv' right='40px'></subparam>

      <subparam id='fourthdiv' right='0px'></subparam>
    </param>

    <param
      useCommonParser='true'
      direction='rightTop'
      anchorOffset='(2,18)'
      anchorOffsetHD='(12,18)'
    >
      <subparam
        id='firstimage'
        margin-right='0px'
        margin-left='10px'
      ></subparam>

      <subparam id='firstdiv' left='104px' top='13px'></subparam>

      <subparam id='seconddiv' left='12px'></subparam>

      <subparam id='labNormalImage' left='40px' top='25px'></subparam>

      <subparam id='thirddiv' left='40px'></subparam>

      <subparam id='fourthdiv' left='0px'></subparam>
    </param>

    <param
      useCommonParser='true'
      direction='leftBottom'
      anchorOffset='(2,18)'
      anchorOffsetHD='(12,10)'
    >
      <subparam
        id='firstimage'
        margin-right='10px'
        margin-left='0px'
      ></subparam>

      <subparam id='firstdiv' right='104px' top='27px'></subparam>

      <subparam id='seconddiv' right='12px'></subparam>

      <subparam id='labNormalImage' right='40px' top='40px'></subparam>

      <subparam id='thirddiv' right='40px'></subparam>

      <subparam id='fourthdiv' right='0px'></subparam>
    </param>

    <param
      useCommonParser='true'
      direction='rightBottom'
      anchorOffset='(2,18)'
      anchorOffsetHD='(12,10)'
    >
      <subparam
        id='firstimage'
        margin-right='0px'
        margin-left='10px'
      ></subparam>

      <subparam id='firstdiv' left='104px' top='27px'></subparam>

      <subparam id='seconddiv' left='12px'></subparam>

      <subparam id='labNormalImage' left='40px' top='40px'></subparam>

      <subparam id='thirddiv' left='40px'></subparam>

      <subparam id='fourthdiv' left='0px'></subparam>
    </param>
  </params>

  <div style='display: flex; flex-direction: row'>
    <div style='display: flex; flex-direction: row'>
      <img
        id='firstimage'
        style='width: 204px; height: 136px'
        src='{bgRes}'
      ></img>
    </div>

    <div
      id='firstdiv'
      style='display: flex; width: 92px; height: 92px; justify-content: space-around; align-items: center; flex-direction: column; position: absolute'
    >
      <img
        id='addPopImage'
        style='width: 92px; height: 92px'
        src='{addPopResId}'
      ></img>

      <img
        id='addNormalImage'
        style='display: {addImageDisplay}; width: 68px; height: 68px; top: 12px; right: 12px; align-items: center; position: absolute'
        src='{addSubResId}'
      ></img>

      <div
        id='seconddiv'
        style='display: {addSpeedDisplay}; width: 68px; height: 68px; justify-content: center; align-items: center; top: 12px; position: absolute'
      >
        <label
          id='addSpeed'
          style='font-weight: bold; font-size: {addFontSize}; color: #222222'
          text='{addMaxSpeed}'
        ></label>
      </div>
    </div>

    <div
      id='fourthdiv'
      style='display: flex; justify-content: space-around; align-items: center; flex-direction: column; top: 0px; position: absolute'
    >
      <img
        id='labPopImage'
        style='width: 142px; height: 136px'
        src='{popResId}'
      ></img>

      <img
        id='labNormalImage'
        style='display: {imageDisplay}; width: 68px; height: 68px; align-items: center; position: absolute'
        src='{subResId}'
      ></img>

      <div
        id='thirddiv'
        style='display: {speedDisplay}; width: 68px; height: 68px; justify-content: center; align-items: center; top: 25px; position: absolute'
      >
        <label
          id='labSpeed'
          style='font-weight: bold; font-size: {fontSize}; color: #222222'
          text='{maxspeed}'
        ></label>
      </div>
    </div>
  </div>
</xml>
